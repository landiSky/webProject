{
  "name": "teleinfo-design-vue3",
  "description": "Teleinfo Design for Vue3",
  "version": "1.0.0",
  "private": true,
  "author": "Teleinfo Team",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "mock": "vite --config ./config/vite.config.dev.ts --mode mock",
    "dev": "vite --config ./config/vite.config.dev.ts",
    "build": "vue-tsc --noEmit && vite build --config ./config/vite.config.prod.ts",
    "lint-staged": "npx lint-staged",
    "tsc": "vue-tsc --noEmit",
    "fix-memory-limit": "cross-env LIMIT=8192 increase-memory-limit"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "stylelint --fix",
      "prettier --write",
      "eslint --fix"
    ],
    "*.{less,css}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@icon-park/vue-next": "1.3.6",
    "@tele-design/web-vue": "1.0.23-beta.4",
    "@vue-office/docx": "^1.6.2",
    "@vue-office/excel": "^1.7.8",
    "@vue-office/pdf": "^2.0.2",
    "@vue/runtime-core": "^3.2.45",
    "@vueuse/core": "^7.3.0",
    "axios": "^0.24.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "mitt": "^3.0.1",
    "vuedraggable": "4.1.0",
    "html2canvas": "^1.4.1",
    "increase-memory-limit": "^1.0.7",
    "jspdf": "^2.5.1",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.9",
    "sm-crypto": "^0.3.13",
    "vant": "^2.13.2",
    "vue": "^3.2.16",
    "vue-router": "^4.2.2",
    "vue3-seamless-scroll": "^2.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^12.0.1",
    "@rollup/plugin-dynamic-import-vars": "^1.4.2",
    "@types/lodash": "^4.14.177",
    "@types/mockjs": "^1.0.4",
    "@types/node": "^18.11.10",
    "@types/nprogress": "^0.2.0",
    "@types/sm-crypto": "^0.3.3",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^4.12.0",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^11.2.6",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.1",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "~4.4.4",
    "vite": "^2.6.4",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-svg-icons": "^1.1.0",
    "vite-plugin-vue-setup-extend": "^0.3.0",
    "vite-svg-loader": "^3.1.0",
    "vue-tsc": "^0.3.0"
  }
}
